{
  "expo": {
    "name": "Glyde_S",
    "slug": "Glyde_S",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Glyde.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "glyde"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Glyde.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.osama92.Glyde_S",
      "config": {
        "googleMaps": {
          "apiKey" : "AIzaSyB2eJbCGeuoY2t6mvf8SjiYk0QPrevGKi0"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(Glyde_S) to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Glyde.png",
          "color": "#ffffff",
          "sounds": ["./assets/sounds/notifSound.wav"],
          "notification": {
            "iosDisplayInForeground": true,
            "androidMode": "default",
            "androidCollapsedTitle": "$(Glyde Systems) Notification"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(Glyde Systems) to access your photos."
        }
      ],
      [
        "expo-dev-client",
        {
          "ios": {
            "bundleIdentifier": "glyde"
          },
          "android": {
            "package": "com.osama92.Glyde_S"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/expo-gh-pages"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "31bc02c2-d62a-48f1-961c-28b6cfc8f93e"
      }
    }
  }
}
